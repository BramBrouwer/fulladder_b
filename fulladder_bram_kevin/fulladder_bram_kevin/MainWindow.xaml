<Window x:Class="fulladder_bram_kevin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fulladder_bram_kevin"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="888" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="button" Content="Open circuit" HorizontalAlignment="Left" Margin="404,521,0,0" VerticalAlignment="Top" Width="134" Height="22" Click="button_Click"/>
        <Label x:Name="label" Content="Log" HorizontalAlignment="Left" Margin="10,13,0,0" VerticalAlignment="Top" Width="191" FontWeight="Bold" FontSize="15pt" BorderBrush="Black" BorderThickness="1px" HorizontalContentAlignment="Center"/>
        <TextBox x:Name="logBody" HorizontalAlignment="Left" Height="504" Margin="10,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="191" TextChanged="logBody_TextChanged" IsReadOnly="True" BorderBrush="Black" BorderThickness="1px" TextAlignment="Center" />
        <Canvas x:Name="mainCanvas" HorizontalAlignment="Left" Height="499" Margin="206,0,0,0" VerticalAlignment="Top" Width="666"/>

    </Grid>
</Window>
